{
  "intent-type": "{{ intent_type }}",
  "version": "1",
  "build": "-",
  "author": "{{ author }}",
  "mapping-engine": "js-scripted-graal",
  "label": ["DevOpsNSP"],
  "priority": 50,
  "return-config-as-json": true,
  "support-nested-type-in-es": false,
  "supports-network-state-suspend": true,
  "target-component": [
    {
      "order": 1,
      "name": "id",
      "i18n-text": "Identifier",
      "value-type": "STRING",
      "length": "1..32",
      "pattern": "[a-zA-Z][a-zA-Z0-9_]*",
      "pattern-error-message": "Identifier using up to 32 letters and digits"
    }
  ],
  "supports-health": "never",
  "lifecycle-state": "released",
  "live-state-retrieval": true
}